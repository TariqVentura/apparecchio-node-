<div class="text">Categorias</div>
<main class="container">
    <div class="container-fluid py-3 border-bottom d-none d-lg-block d-print-block">
    </div>
    <div class="row">
        <div class="col-6 d-flex flex-nowrap">
            <form method="post" id="search-form">
                <div class="input-field  flex-nowrap col-12">
                    <img src="https://img.icons8.com/ios-filled/32/null/search--v1.png" />
                    <input id="search" type="text" name="Buscar " required>
                    <button type="submit" class="btn waves-effect green tooltipped" data-tooltip="Buscar">
                        <img src="https://img.icons8.com/glyph-neue/40/null/ok--v1.png" />
                    </button>
                </div>
            </form>
        </div>
        <div class="col-2">
            <div class="input-field center-align col-2">
                <button class="border-0 bg-light" onclick="openCreate()"><img
                        src="https://img.icons8.com/ios-glyphs/40/null/add--v1.png" /></button>
            </div>
        </div>
    </div>
    <div class="container-fluid py-3 border-bottom d-none d-lg-block d-print-block"></div>
    <div class="container">
        <div class="row">
            <div class="col-6">
                <div class="card mb-3">
                    <div class="row g-0">
                        <div class="col-md-4">
                            <img src="/images/refrigerador.png" width="200" class="img-fluid rounded-start"
                                alt="Refrigerador">
                        </div>
                        <div class="col-md-5">
                            <div class="card-body">
                                <h5 class="card-title">Refrigeradores</h5>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <button class="border-0 bg-light" onclick="openUpdate()"><img
                                    src="https://img.icons8.com/ios-glyphs/32/null/refresh--v1.png" /></button><br><br>
                            <button class="border-0 bg-light" onclick="openDelete()"><img
                                    src="https://img.icons8.com/ios-filled/30/null/waste.png" /></button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-6">
                <div class="card mb-3">
                    <div class="row g-0">
                        <div class="col-md-4">
                            <img src="/images/cocina.png" width="135" class="img-fluid rounded-start" alt="Cocina">
                        </div>
                        <div class="col-md-6">
                            <div class="card-body">
                                <h5 class="card-title">Cocinas</h5>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <button class="border-0 bg-light" onclick="openUpdate()"><img
                                    src="https://img.icons8.com/ios-glyphs/32/null/refresh--v1.png" /></button><br><br>
                            <button class="border-0 bg-light" onclick="openDelete()"><img
                                    src="https://img.icons8.com/ios-filled/30/null/waste.png" /></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


</main>
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js"
    integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p"
    crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js"
    integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF"
    crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/dashboard/categoria.js"></script>